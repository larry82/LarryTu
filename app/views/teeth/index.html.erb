<p id="notice"><%= notice %></p>

<h1>Listing Teeth</h1>

<table>
  <thead>
    <tr>
      <th>Pic name</th>
      <th>Times</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teeth.each do |tooth| %>
      <tr>
        <td><%= tooth.pic_name %></td>
        <td><%= tooth.times %></td>
        <td><%= link_to 'Show', tooth %></td>
        <td><%= link_to 'Edit', edit_tooth_path(tooth) %></td>
        <td><%= link_to 'Destroy', tooth, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tooth', new_tooth_path %>
